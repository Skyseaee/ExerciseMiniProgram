/* pages/component/notice/notice.wxss */
.notice{
    width: 100%;
    height: 100%;
  }
  .mask{
    position:fixed;
    top:0;
    right:0;
    left:0;
    bottom:0;
    background-color:rgba(0, 0, 0, 0.5);
    z-index: 99;
  }
  .notice-box{
    width: 100%;
    z-index: 1000;
    position:fixed;
    margin-top: 70rpx;
  }
  .pictrue{
    width: 100%;
    text-align: center;
  }
  .pictrue image{
    width: 640rpx;
    height: 480rpx;
  }
  .word{
    font-size: 34rpx;
    color: #fff;
    text-align: center;
    z-index: 1000;
  }
  .close{
    width: 70%;
    height: 80rpx;
    line-height: 80rpx;
    color: #000;
    background: #fff;
    border-radius: 50rpx;
    text-align: center;
    font-weight: bold;
    margin-left: 15%;
    margin-top: 60rpx;
  }