/* pages/my/my.wxss */
page {
    height: 100%;
    background-color: #f3f3f3;
}

button::after {
    border: none;
}

.service-button {
    height: 100%;
    position: absolute;
    width: 100%;
    left: 0;
    top: 0px;
    background-color: transparent;
}

.header {
    padding-top: 150rpx;
}

.btn {
    background-color: transparent;
    padding-top: 150rpx;
}

.topImage {
    width: 100%;
    height: 380rpx;
    position: absolute;
    z-index: 0;
    margin-top: 60rpx;
}

.topBG {
    background-color: #46C6BA;
    height: 450rpx;
    position: relative;
    width: 100%;
}

.head {
    display: flex;
    width: 130rpx;
    height: 130rpx;
    border-radius: 50%;
    border: 6rpx #ffffff solid;
    margin: 0 auto;
    z-index: 3;
    position: relative;
}

.name {
    font-size: 32rpx;
    text-align: center;
    margin-top: 30rpx;
    z-index: 3;
    color: #ffffff;
    position: relative;
}

.cell-g {
    margin-top: 10rpx;
}

.bottom {
    padding-bottom: 30rpx;
}