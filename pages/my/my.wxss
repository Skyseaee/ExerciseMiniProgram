/* pages/my/my.wxss */
page {
    height: 100%;
    background-color: #f3f3f3;
}

button::after {
    border: none;
}

.service-button {
    height: 100%;
    position: absolute;
    width: 100%;
    left: 0;
    top: 0px;
    background-color: transparent;
}

.header {
  padding-top: 150rpx;
}

.btn {
  background-color: transparent;
  padding-top: 150rpx;
}

.topImage {
    width: 100%;
    height: 380rpx;
    position: absolute;
    z-index: 0;
    margin-top: 60rpx;
}

.topBG {
    background-color: #fff;
    height: 450rpx;
    position: relative;
    width: 100%;
}

.head {
    display: flex;
    width: 130rpx;
    height: 130rpx;
    border-radius: 50%;
    border: 6rpx #78a6eb solid;
    margin: 0 auto;
    z-index: 3;
    position: relative;
}

.name {
    font-size: 32rpx;
    text-align: center;
    margin-top: 30rpx;
    z-index: 3;
    color: #222;
    position: relative;
}

.cell-g {
  margin-top: 24rpx;
  width: 95%;
  margin: 20rpx auto;
  box-shadow: 0px 4px 8px 0px rgba(219,220,233,0.5);
  border-radius: 8px;
  display: flex;
  flex-direction: column;
}

.item {
  display: flex;
  width: 92%;
  height: 90rpx;
  padding: 0 29rpx;
  justify-content: space-between;
  align-items: center;
  background-color: #fff;
}

.inside {
  display: flex;
  align-items: center;
}

.texts {
  font-size: 16px;
  color: #222;
  font-weight: 500;
  margin-left: 20rpx;
}

.icon {
  height: 23px;
  width: 23px;
}

.small-icon {
  height: 18px;
  width: 18px;
}

.bottom {
    padding-bottom: 30rpx;
}

.bottoms{
  padding: 12rpx;
}

.contract {
  width: 100%;
  background-color: #fff;
  padding: 0;
}